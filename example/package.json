{
  "name": "@webdoc/example",
  "version": "1.0.0",
  "description": "Example documented repository using webdoc",
  "keywords": [
    "example",
    "webdoc"
  ],
  "author": "Shukant Pal <shukantpal@outlook.com>",
  "homepage": "https://github.com/SukantPal/webdoc/tree/master/packages/example#readme",
  "license": "MIT",
  "main": "lib/example.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SukantPal/webdoc.git"
  },
  "scripts": {
    "unit-test": "",
    "build": "webdoc --tutorials ./tutorials --site-root example-documentation",
    "build-next": "cd .. && webdoc && cd example",
    "build-pixi-api": "cd ../../pixi-api && webdoc --site-root docs && cd ../webdoc/example",
    "build-pixi-api-prod": "cd ../../pixi-api && webdoc --site-root pixi-api && cd ../webdoc/example",
  },
  "bugs": {
    "url": "https://github.com/SukantPal/webdoc/issues"
  },
  "dependencies": {
    "@webdoc/cli": "^1.1.3"
  },
  "gitHead": "a2866c1a42f60180a74add7bc1b0f42d8bb16330",
  "devDependencies": {
    "flow-typed": "^3.2.1",
    "@webdoc/eslint-config": "^1.0.0"
  }
}
