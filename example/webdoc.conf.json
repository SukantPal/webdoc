{
  "source": {
      "include": "./src/**/*"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "template": {
    "readme": "./README.md",
    "outputSourceFiles": true,
    "meta": {
      "og:title": "webdoc",
      "og:description": "The documentation for webdoc, by webdoc!",
      "title": "webdoc"
    }
  },
  "opts": {
    "destination": "example-documentation",
    "import": ["./gl-matrix.api.json"],
    "export": "./example-documentation/webdoc.api.json"
  }
}
