{
    "source": {
        "include": [
          "packages/webdoc-cli/src/**/*.js",
          "packages/webdoc-externalize/src/**/*.js",
          "packages/webdoc-model/src/**/*.js",
          "packages/webdoc-parser/src/**/*.js"
        ]
    },
    "plugins": [
      "plugins/markdown"
    ],
    "opts": {
      "destination": "example/next-docs"
    },
    "template": {
      "readme": "./README.md",
      "outputSourceFiles": true
    }
}
